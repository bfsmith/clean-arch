{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Keycloak": {
    "Authority": "https://localhost:8080/realms/cleanarch",
    "Audience": "setme",
    "RequireHttpsMetadata": true,
    "Scopes": [
      "email",
      "profile"
    ]
  },
  "OpenTelemetry": {
    "ServiceName": "cleanarch-api",
    "ServiceVersion": "1.0.0",
    "RequestPathsToIgnore": [
      "/healthz"
    ]
  }
}
